.project-right-price {
  align-items: center;
}
.project-right-price text:first-child {
  color: black;
}
.project-right-price text:last-child {
  color: #FF0074;
  font-size: 120%;
}
.payment-type-title {
  padding: 20rpx;
  color: #A19FA0;
}
.payment-type-cell {
    padding: 20rpx;
    background-color: white;
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-top: solid 2rpx #F7F7F7;
}
.payment-type-cell-hd {
    display: flex;
    align-items: center;
}
.payment-type-cell-hd image {
    width: 40rpx;
    height: 40rpx;
    padding-right: 10rpx;
}
.payment-type-cell-ft {
    width: 65%;
    display: flex;
    justify-content: flex-end;
}
.payment-type-cell-ft-circle {
    width: 55rpx;
    height: 55rpx;
    border: solid 2rpx #F7F7F7;
    border-radius: 100%;
}
.circle-on {
  background-image: url("https://www.iumer.cn/umer/css/image/wechat/app/selected-round@2x.png");
  background-size: 80rpx;
  background-repeat: no-repeat;
  background-position: center;
}
#btn-area {
    margin-top: 20rpx;
}
.btn-pay {
    width: 100%;
    background-color: #FF0074;
    color: white;
    padding: 30rpx 0;
}